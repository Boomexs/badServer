<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.boomexs.binarytree.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <HBox prefHeight="36.0" prefWidth="200.0">
         <children>
            <TextField fx:id="cmd" />
            <Button fx:id="send" mnemonicParsing="false" text="Send" />
         </children>
      </HBox>
      <TextFlow fx:id="flowText" prefHeight="200.0" prefWidth="200.0" />
   </children>
</VBox>
